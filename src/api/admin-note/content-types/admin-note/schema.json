{
  "kind": "collectionType",
  "collectionName": "admin_notes",
  "info": {
    "singularName": "admin-note",
    "pluralName": "admin-notes",
    "displayName": "AdminNote"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Note": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "Decision": {
      "type": "enumeration",
      "enum": [
        "Approve",
        "Reject",
        "Request More Information"
      ]
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    }
  }
}
