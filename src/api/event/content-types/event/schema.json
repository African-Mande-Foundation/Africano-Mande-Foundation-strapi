{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "Cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Date": {
      "type": "datetime"
    },
    "Location": {
      "type": "string"
    },
    "seats": {
      "type": "biginteger"
    },
    "seats_remaining": {
      "type": "biginteger"
    },
    "users_permissions_users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "upcoming",
        "ongoing",
        "completed",
        "cancelled"
      ]
    }
  }
}
